        "path": "/events/83f9262f-28f1-4703-ab1a-8cfd9e8249c9",
        "headers": {
          "Accept": "application/json"
        },
        "matchingRules": {
          "$.path": {
            "match": "regex",
            "regex": "^\\/events\\/[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
